{
  "name": "ma",
  "description": "Utilize Mingle API to improve some of the Mingle Interface",
  "version": "0.1",
  "permissions":
  [ "activeTab", "tabs", "notifications", "*://*/*" ],
  "background": {
    "scripts": [ "js/background.js" ]
  },
  "icons": {
    "16": "img/ma-icon-16.png",
    "48": "img/ma-icon-48.png"
  },
  "web_accessible_resources": [
    "img/ma-icon-48.png",
    "html/main.html"
  ],
  "browser_action": {
    "default_icon": "img/ma-icon-48.png",
    "default_title": "MA"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://*.googleapis.com;object-src 'self'",
  "manifest_version": 2
}