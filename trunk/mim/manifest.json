{
  "name": "M in M",
  "description": "Playground for google extentions",
  "version": "0.1",
  "permissions": 
	  [ "tabs", "webRequest", "webRequestBlocking", "*://*/*" ],
  "background": {
    "scripts": [ "js/background.js" ]
  },
  "icons": { 
	  "16": "img/matrix-icon-16.png",
      "48": "img/matrix-icon-48.png"
  },
  "browser_action": {
    "default_icon": "img/matrix-icon-48.png",
    "default_title": "M in M"
  },
  "manifest_version": 2
}